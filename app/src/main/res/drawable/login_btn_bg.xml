<?xml version="1.0" encoding="utf-8"?>
<!-- gradient effect for login button-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient android:centerColor="#6DC4F6"
                android:endColor="#77D4F3"
                android:startColor="#59A4FF"
                />
            <size android:width="100dp" android:height="44dp"></size>
        </shape>

    </item>
</selector>